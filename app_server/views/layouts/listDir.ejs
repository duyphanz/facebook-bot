<%if(directories){%>
    <%directories.forEach( (directory, index) => {%>
        <span>
            <button class="ui primary basic button" id="<%= directory%><%= index%>" onclick="reloadLink('<%= directory%>')">
                <%= directory%>
            </button>

        </span>
        <%})}%>
            <%if(message){%>
                <div class="ui warning message transition">
                   
                    <div class="header">
                        <%= message%>
                    </div>

                </div>
                <%}%>